		},
		"AzureSqlDatabase4_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase4'"
		},
		"azurebloblink_copy_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azurebloblink_copy'"
		},
		"azurelink_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azurelink_blob'"
		},
		"dipertment1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'dipertment1'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gen2datalake7132023.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://09272023datalakestorage.dfs.core.windows.net/"
		},
