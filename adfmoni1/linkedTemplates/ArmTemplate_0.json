		},
		{
			"name": "[concat(parameters('factoryName'), '/azurebloblink_copy')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('azurebloblink_copy_connectionString')]"
				}
			},
			"dependsOn": []
