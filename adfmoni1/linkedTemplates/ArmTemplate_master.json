{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfmoni1"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBloblink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBloblink'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'",
			"defaultValue": "Integrated Security=True;Data Source=MAHI\\SQLEXPRESS;Initial Catalog=azure"
		},
		"SqlServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer1'"
		},
		"employefile_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'employefile'"
		},
		"samplefile_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'samplefile'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gen2datalake7132023.dfs.core.windows.net/"
		},
		"SqlServer1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "MAHI\\tapas"
		},
		"employefile_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\Sample Files/CSV/"
		},
		"employefile_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "tapakmondal@hotmail.com"
		},
		"samplefile_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\Users\\tapas\\OneDrive\\Desktop\\\\Sample Files-20230714T150346Z-001.zip\\Sample Files\\CSV"
		},
		"samplefile_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "tapaskmondal@hotmail.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfmoni1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBloblink_connectionString": {
						"value": "[parameters('AzureBloblink_connectionString')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"SqlServer1_password": {
						"value": "[parameters('SqlServer1_password')]"
					},
					"employefile_password": {
						"value": "[parameters('employefile_password')]"
					},
					"samplefile_password": {
						"value": "[parameters('samplefile_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"SqlServer1_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer1_properties_typeProperties_userName')]"
					},
					"employefile_properties_typeProperties_host": {
						"value": "[parameters('employefile_properties_typeProperties_host')]"
					},
					"employefile_properties_typeProperties_userId": {
						"value": "[parameters('employefile_properties_typeProperties_userId')]"
					},
					"samplefile_properties_typeProperties_host": {
						"value": "[parameters('samplefile_properties_typeProperties_host')]"
					},
					"samplefile_properties_typeProperties_userId": {
						"value": "[parameters('samplefile_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}